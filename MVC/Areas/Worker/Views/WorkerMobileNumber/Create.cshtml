@model OmSaiModels.Worker.WorkerMobileNumbersModel

@{
    ViewData["Title"] = "Create WorkerMobileNumber";
}

<div class="app-content-header">
    <!--begin::Container-->
    <div class="container-fluid">
        <!--begin::Row-->
        <div class="row">
            <div class="col-sm-6">
                <h3 class="mb-0">
                    Add New WorkerMobileNumber
                </h3>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-end">
                    <li class="breadcrumb-item"><a href="#">Master</a></li>
                    <li class="breadcrumb-item"><a asp-area="Worker" asp-controller="WorkerMobileNumber" asp-action="Index">Roles</a></li>
                    <li class="breadcrumb-item active" aria-current="page">
                        Create
                    </li>
                </ol>
            </div>
        </div> <!--end::Row-->
    </div> <!--end::Container-->
</div> <!--end::App Content Header--> <!--begin::App Content-->


<div class="app-content">
    <!--begin::Container-->
    <div class="container-fluid">
        <partial name="~/Views/shared/_alertpartial.cshtml" />
        <div class="row">
            <div class="col-lg-4 col-md-4 col-sm-12">
                <div class=" mb-4">
                    <div class="card">
                        <div class="card-body">
                            <form asp-action="Create" method="post">

                                <div class="form-group mb-3">
                                    <label for="workerId">Worker Id</label>
                                    <input type="text" asp-for="WorkerId" id="workerId" class="form-control" />
                                </div>
                                <div class="form-group mb-3">
                                    <label for="mobileNumber">Mobile Number</label>
                                    <input type="text" asp-for="MobileNumber" id="mobileNumber" class="form-control" />
                                </div>


                                <button type="submit" class="btn btn-outline-primary">Create</button>
                                <a class="btn btn-outline-warning" asp-area="Worker" asp-controller="WorkerMobileNumber" asp-action="Index"> Cancel</a>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
